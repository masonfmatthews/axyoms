<h1>
  <div class="header-buttons">
    <%= link_to '<i class="glyphicon glyphicon-plus"></i> New Student'.html_safe,
        new_student_path, class: "btn btn-default" %>
  </div>
  Students
</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Avg Score</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <%= render @students %>
  </tbody>
</table>

<%= will_paginate %>
