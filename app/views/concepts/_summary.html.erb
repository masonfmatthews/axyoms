<% if @concept %>
  <h3 id="units">
    <span class="label label-primary">Unit 1</span>
    <span class="label label-default">Unit 3</span>
  </h3>
  <h1><%= @concept.name %></h1>
  <div class="row">
    <div class="col-md-6">
      <% unless @concept.description.blank? %>
        <h3>Description</h3>
        <p><%= @concept.description %></p>
      <% end %>
      <% @concept.implementations.each do |i| %>
        <h3>Implementation: <%= i.name %></h3>
        <p><%= i.description %></p>
      <% end %>
    </div>
    <div class="col-md-6">
      <h3>Assignments</h3>
      <p class="text-muted">None found.</p>
      <h3>References</h3>
      <p class="text-muted">None found.</p>
    </div>
  </div>
<% end %>
